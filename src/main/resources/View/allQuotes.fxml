<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AllQuotes">
    <AnchorPane layoutY="118.0" prefHeight="282.0" prefWidth="600.0">
        <TableView fx:id="table" prefHeight="282.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="quote" prefWidth="249.0" text="цитата" />
                <TableColumn fx:id="teacher" minWidth="0.0" prefWidth="111.0" text="преподаватель" />
                <TableColumn fx:id="subject" prefWidth="97.0" text="предмет" />
                <TableColumn fx:id="date" prefWidth="88.0" text="дата" />
            <TableColumn fx:id="idColumn" prefWidth="88.0" text="id" />
            </columns>
        </TableView>
    </AnchorPane>
    <Button layoutX="48.0" layoutY="50.0" mnemonicParsing="false" onAction="#changeSceneToMenu" text="меню" />
    <Button layoutX="145.0" layoutY="50.0" mnemonicParsing="false" onAction="#showAllQuotes" text="все записи" />
    <Button layoutX="242.0" layoutY="50.0" mnemonicParsing="false" onAction="#showUserQuotes" text="мои записи" />
</AnchorPane>
