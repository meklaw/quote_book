<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.Authorize">
   <PasswordField fx:id="passwordField" layoutX="139.0" layoutY="200.0" prefHeight="0.0" prefWidth="122.0"
                  promptText="Пароль"/>
   <Button layoutX="174.0" layoutY="241.0" mnemonicParsing="false" onAction="#signIn" text="Войти"/>
   <TextField fx:id="loginField" layoutX="139.0" layoutY="156.0" prefHeight="25.0" prefWidth="122.0"
              promptText="Логин"/>
   <Text layoutX="145.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Авторизация">
      <font>
         <Font size="18.0"/>
      </font>
   </Text>
   <Button layoutX="156.0" layoutY="285.0" mnemonicParsing="false" onAction="#signUp" text="Регистрация"/>
   <Button layoutX="173.0" layoutY="334.0" mnemonicParsing="false" onAction="#menu" text="Меню"/>
</AnchorPane>
